    <TextView
        android:id="@+id/row_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:lines="1"
        android:text="ROW_ID"
        android:textSize="24dp"
        >
    </TextView>

    <TextView
        android:id="@+id/message_id"
        android:layout_width="match_parent"
